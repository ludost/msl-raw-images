	    <listener-class>org.stwerff.mslraws.images.InitListener</listener-class>
	</listener>
	<security-constraint>
		<web-resource-collection>
			<web-resource-name>Memo</web-resource-name>
			<url-pattern>/memo</url-pattern>
		</web-resource-collection>
		<auth-constraint>
			<role-name>admin</role-name>
		</auth-constraint>
	</security-constraint>
