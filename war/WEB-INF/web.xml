    <servlet>
	    <servlet-name>landingPage</servlet-name>
	    <servlet-class>org.stwerff.mslraws.LandingPageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>landingPage</servlet-name>
	    <url-pattern>/landing</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>collector</servlet-name>
		<servlet-class>org.stwerff.mslraws.MslCollectorServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>collector</servlet-name>
		<url-pattern>/collector</url-pattern>
	</servlet-mapping>
	<servlet>
	    <servlet-name>Mailer</servlet-name>
	    <servlet-class>org.stwerff.mslraws.MailServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>Mailer</servlet-name>
	    <url-pattern>/mailer</url-pattern>
	</servlet-mapping>
		<servlet>
		<servlet-name>JSON</servlet-name>
		<servlet-class>com.chap.memo.memoNodes.servlet.MemoJSONServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>JSON</servlet-name>
		<url-pattern>/json</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Memo</servlet-name>
		<servlet-class>com.chap.memo.memoNodes.servlet.MemoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Memo</servlet-name>
		<url-pattern>/memo</url-pattern>
	</servlet-mapping>	
	<servlet>
		<servlet-name>MemoTest</servlet-name>
		<servlet-class>com.chap.memo.memoNodes.servlet.MemoTestServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MemoTest</servlet-name>
		<url-pattern>/test</url-pattern>
	</servlet-mapping>	
		<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
